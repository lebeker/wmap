{
    "extends": "../tslint.json",
    "rules": {
        "prefer-const": false,
        "quotemark": [
            true,
            "double"
        ],
        "no-bitwise": false,
        "curly": false,
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "wmap",
            "kebab-case"
        ]
    }
}
